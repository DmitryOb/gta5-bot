#autorun

$y2_answer = 485
$y_otv_CORR = 700

$x_answer_CORR = 1270
$x_otv_CORR = 1200

waitms(4)

move($x_answer_CORR, $y2_answer)
waitms(204)
  lDown($x_answer_CORR, $y2_answer)
waitms(96)
  lUp($x_answer_CORR, $y2_answer)
waitms(300)

move($x_otv_CORR, $y_otv_CORR)
waitms(204)
  lDown($x_otv_CORR, $y_otv_CORR)
waitms(96)
  lUp($x_otv_CORR, $y_otv_CORR)
waitms(300)

HALT(1)